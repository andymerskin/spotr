---
import { examples } from '../../data/examples';

// Redirect to first example with default framework
const defaultExample = examples[0];
const redirectUrl = `${import.meta.env.BASE_URL}examples/${defaultExample.slug}/react`;
---

<meta http-equiv="refresh" content={`0;url=${redirectUrl}`} />
<script
  is:inline
  set:html={`window.location.href = ${JSON.stringify(redirectUrl)};`}
/>
