{
  "name": "spotr",
  "version": "1.0.0-alpha.1",
  "description": "A powerful and minimal client-side fuzzy search library",
  "author": "Andy Merskin <andymerskin@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/andymerskin/spotr",
  "keywords": [
    "search",
    "fuzzy",
    "client-side",
    "levenshtein",
    "filter"
  ],
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.js",
      "require": "./dist/vue.cjs"
    },
    "./svelte": {
      "types": "./dist/svelte.d.ts",
      "import": "./dist/svelte.js",
      "require": "./dist/svelte.cjs"
    },
    "./solid": {
      "types": "./dist/solid.d.ts",
      "import": "./dist/solid.js",
      "require": "./dist/solid.cjs"
    },
    "./preact": {
      "types": "./dist/preact.d.ts",
      "import": "./dist/preact.js",
      "require": "./dist/preact.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "typecheck:all": "bun run typecheck && cd examples/react && bun run typecheck && cd ../vue && bun run typecheck && cd ../solid && bun run typecheck && cd ../preact && bun run typecheck",
    "lint": "eslint src/ examples/shared examples/react/src examples/vue/src examples/svelte/src examples/solid/src examples/preact/src",
    "sync-examples": "bun run scripts/sync-examples.ts",
    "prepare": "husky"
  },
  "peerDependencies": {
    "react": ">=18",
    "vue": ">=3",
    "svelte": ">=4",
    "solid-js": ">=1",
    "preact": ">=10"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "solid-js": {
      "optional": true
    },
    "preact": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "eslint": "^10.0.0",
    "husky": "^9.1.7",
    "preact": "^10.27.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "solid-js": "^1.9.0",
    "svelte": "^5.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.5",
    "vue": "^3.5.13"
  },
  "packageManager": "bun@1.1.38"
}
